# Nombre de la aplicación
spring.application.name=backend

# Configuración de la base de datos
spring.datasource.url=jdbc:mysql://localhost:3306/camyo_db_test?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
spring.datasource.username=root
spring.datasource.password=YOUR_PASSWORD_HERE

# Driver de MySQL
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

# Configuración de JPA (Hibernate)
spring.jpa.database-platform=org.hibernate.dialect.MySQL8Dialect
spring.jpa.hibernate.ddl-auto=create-drop
spring.jpa.show-sql=true


# JWT Config
camyo.app.jwtSecret=CAMYO_APP_JWT_SECRET_TEST
camyo.app.jwtExpirationMs=86400000
AES_KEY=your_secret_key_here
AES_IV=your_secret_key_here